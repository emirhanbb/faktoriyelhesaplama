#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {
	
	int a;
	int fact= 1;
	
	printf("Faktoriyel hesabi yapilacak sayiyi girin:");
	scanf("%d",&a);
	while (a != 0){
		
	printf("%d\n",a);
	fact= fact *a;
	a--;

}
	printf("Faktoriyel: %d",fact);
	
	return 0;
}
